{
  "name": "svg-slimming",
  "version": "1.4.1",
  "description": "svg slimming",
  "main": "dist/svg-slimming.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --color --profile --bail --display-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --color --profile --bail --display-modules",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "dist/*.js",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "keywords": [
    "svg",
    "xml",
    "optimization",
    "minimization",
    "parse",
    "svg-slimming",
    "xml-parser",
    "javascript",
    "nodejs"
  ],
  "author": "Wang Feng <benboba@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "css": "^2.2.3",
    "he": "^1.2.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/css": "0.0.31",
    "@types/he": "^1.1.0",
    "@types/mocha": "^5.2.7",
    "@types/ramda": "^0.25.36",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^3.2.4",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
